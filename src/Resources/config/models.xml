<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Ratepay\RpayPayments\Core\Entity\Definition\RatepayOrderDataDefinition">
            <tag name="shopware.entity.definition" entity="ratepay_order_data"/>
        </service>
        <service id="Ratepay\RpayPayments\Core\Entity\Definition\RatepayOrderLineItemDataDefinition">
            <tag name="shopware.entity.definition" entity="ratepay_order_line_item_data"/>
        </service>
        <service id="Ratepay\RpayPayments\Core\Entity\Definition\RatepayPositionDefinition">
            <tag name="shopware.entity.definition" entity="ratepay_position"/>
        </service>

        <service id="Ratepay\RpayPayments\Core\Entity\Extension\OrderExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="Ratepay\RpayPayments\Core\Entity\Extension\OrderLineItemExtension">
            <tag name="shopware.entity.extension"/>
        </service>
    </services>
</container>
