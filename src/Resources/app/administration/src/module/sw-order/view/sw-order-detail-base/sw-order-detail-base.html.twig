{% block sw_order_detail_base__user_card %}

    {% block ratepay_order_management__container %}
        <ratepay-order-management
            v-if="isOrderPayedWithRatepay"
            @ratepayActionTriggered="onRatepayActionTriggered">
        </ratepay-order-management>
    {% endblock %}

    {% block ratepay_order_history_log__container %}
        <ratepay-order-history-log-grid
            v-if="isOrderPayedWithRatepay"
            ref="ratepayOrderHistory">
        </ratepay-order-history-log-grid>
    {% endblock %}

    {% parent %}

{% endblock %}
