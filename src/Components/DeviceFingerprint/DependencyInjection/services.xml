<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Ratepay\RatepayPayments\Components\DeviceFingerprint\DfpService" public="true">
            <argument type="service" id="Symfony\Component\HttpFoundation\Session\SessionInterface"/>
        </service>

        <service id="Ratepay\RatepayPayments\Components\DeviceFingerprint\Subscriber\DeviceFingerPrintSubscriber" public="true">
            <tag name="kernel.event_subscriber"/>
            <argument id="Ratepay\RatepayPayments\Components\DeviceFingerprint\DfpService" type="service"/>
            <argument id="Shopware\Core\System\SystemConfig\SystemConfigService" type="service"/>
        </service>

    </services>

</container>
